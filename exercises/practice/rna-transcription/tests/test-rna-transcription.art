import.version:3.0.0 {unitt}! 
import {src/rna-transcription}!

describe "RNA Transcription" [
    it "Empty RNA sequence" [
        expects.be:'equal? @[
            express ""
            express toRna ""
        ]
    ]

    it.skip "RNA complement of cytosine is guanine" [
        expects.be:'equal? @[
            express "G"
            express toRna "C"
        ]
    ]

    it.skip "RNA complement of guanine is cytosine" [
        expects.be:'equal? @[
            express "C"
            express toRna "G"
        ]
    ]

    it.skip "RNA complement of thymine is adenine" [
        expects.be:'equal? @[
            express "A"
            express toRna "T"
        ]
    ]

    it.skip "RNA complement of adenine is uracil" [
        expects.be:'equal? @[
            express "U"
            express toRna "A"
        ]
    ]

    it.skip "RNA complement" [
        expects.be:'equal? @[
            express "UGCACCAGAAUU"
            express toRna "ACGTGGTCTTAA"
        ]
    ]
]
