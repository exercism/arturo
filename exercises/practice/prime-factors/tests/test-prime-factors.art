import.version:2.0.1 {unitt}!
import {src/prime-factors}!

describe "Prime Factors" [
    it "no factors" [
        expects.be:'equal? @[[] primeFactors 1]
    ]

    it.skip "prime number" [
        expects.be:'equal? @[[2] primeFactors 2]
    ]
    
    it.skip "another prime number" [
        expects.be:'equal? @[[3] primeFactors 3]
    ]
    
    it.skip "square of a prime" [
        expects.be:'equal? @[[3 3] primeFactors 9]
    ]
    
    it.skip "product of first prime" [
        expects.be:'equal? @[[2 2] primeFactors 4]
    ]
    
    it.skip "cube of a prime" [
        expects.be:'equal? @[[2 2 2] primeFactors 8]
    ]
    
    it.skip "product of second prime" [
        expects.be:'equal? @[[3 3 3] primeFactors 27]
    ]
    
    it.skip "product of third prime" [
        expects.be:'equal? @[[5 5 5 5] primeFactors 625]
    ]
    
    it.skip "product of first and second prime" [
        expects.be:'equal? @[[2 3] primeFactors 6]
    ]
    
    it.skip "product of primes and non-primes" [
        expects.be:'equal? @[[2 2 3] primeFactors 12]
    ]
    
    it.skip "product of primes" [
        expects.be:'equal? @[
            [5 17 23 461]
            primeFactors 901255
        ]
    ]
    
    it.skip "factors include a large prime" [
        expects.be:'equal? @[
            [11 9539 894119]
            primeFactors 93819012551
        ]
    ]
]
