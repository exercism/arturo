import.version:3.0.0 {unitt}! 
import {src/two-fer}!

describe "Two Fer" [
    it "no name given" [
        expects.be:'equal? @[
            express "One for you, one for me."
            express twoFer ""
        ]
    ]

    it.skip "a name given" [
        expects.be:'equal? @[
            express "One for Alice, one for me."
            express twoFer "Alice"
        ]
    ]
    
    it.skip "another name given" [
        expects.be:'equal? @[
            express "One for Bob, one for me."
            express twoFer "Bob"
        ]
    ]
]
