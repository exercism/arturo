import.version:1.1.2 {unitt}!
import {src/nth-prime}!

suite "Nth Prime" [
    test "first prime" [
        result: prime 1
        assert -> 2 = result
    ]

    test.skip "second prime" [
        result: prime 2
        assert -> 3 = result
    ]

    test.skip "sixth prime" [
        result: prime 6
        assert -> 13 = result
    ]

    test.skip "big prime" [
        result: prime 10001
        assert -> 104743 = result
    ]

    test.skip "there is no zeroth prime" [
        result: prime 0
        assert -> false = result
    ]
]
