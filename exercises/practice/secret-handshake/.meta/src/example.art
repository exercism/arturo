commands: function [number][
    res: []
    loop.with:'i reverse digits.base:2 number 'x ->
        if one? x ->
            case i [
                0 -> 'res ++ "wink"
                1 -> 'res ++ "double blink"
                2 -> 'res ++ "close your eyes"
                3 -> 'res ++ "jump"
                4 -> reverse 'res
            ]
    return res
]
